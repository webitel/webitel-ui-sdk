		"typedoc-github-wiki-theme": "^2.1.0",
		"typescript": "^5.9.x",
		"typescript-plugin-css-modules": "^5.1.0",
		"unplugin-auto-import": "^20.3.x",
		"vidstack": "^1.12.13",
		"vite": "^7.3.1",
