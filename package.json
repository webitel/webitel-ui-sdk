{
  "name": "@webitel/ui-sdk",
  "version": "24.12.105",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vitest",
    "lint:js": "eslint",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "prettier --write --list-different . && npm run lint:js --fix",
    "publish-lib": "npm run test:unit && npm run build && npm publish --access public",
    "publish-lib:testless": "npm run build && npm publish --access public",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "tsc": "tsc -p tsconfig.json",
    "update-node": "nvm install --lts && nvm alias default node",
    "i": "npm install"
  },
  "main": "./dist/@webitel/ui-sdk.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/@webitel/ui-sdk.mjs",
      "require": "./dist/@webitel/ui-sdk.cjs"
    },
    "./store": {
      "import": "./src/store/new/index.js",
      "require": "./src/store/new/index.js"
    },
    "./enums": {
      "import": "./src/enums/index.js",
      "require": "./src/enums/index.js"
    },
    "./components": {
      "import": "./src/components/index.js",
      "require": "./src/components/index.js"
    },
    "./scripts": {
      "import": "./src/scripts/index.js",
      "require": "./src/scripts/index.js"
    },
    "./dist*": {
      "import": "./dist*"
    },
    "./src/api*": {
      "import": "./src/api*"
    },
    "./src/locale*": {
      "import": "./src/locale*"
    },
    "./src/composables*": {
      "import": "./src/composables*"
    },
    "./src/css*": {
      "import": "./src/css*"
    },
    "./src/components*": {
      "import": "./src/components*"
    },
    "./src/directives*": {
      "import": "./src/directives*"
    },
    "./src/enums*": {
      "import": "./src/enums*"
    },
    "./src/mixins*": {
      "import": "./src/mixins*"
    },
    "./src/scripts*": {
      "import": "./src/scripts*"
    },
    "./src/validators*": {
      "import": "./src/validators*"
    },
    "./src/modules*": {
      "import": "./src/modules*"
    },
    "./src/plugins*": {
      "import": "./src/plugins*"
    },
    "./src/store*": {
      "import": "./src/store*"
    },
    "./src/tests*": {
      "import": "./src/tests*"
    }
  },
  "files": [
    "dist/*",
    "src/api/*",
    "src/assets/*",
    "src/locale/*",
    "src/composables/*",
    "src/css/*",
    "src/components/*",
    "src/directives/*",
    "src/enums/*",
    "src/mixins/*",
    "src/scripts/*",
    "src/validators/*",
    "src/modules/*",
    "src/plugins/*",
    "src/store/*",
    "src/tests/*",
    "src/install.ts",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@morev/vue-transitions": "^3.0.5",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^4.5.1",
    "@vueuse/components": "^11.2.0",
    "@webitel/styleguide": "^24.12.18",
    "autosize": "^6.0.1",
    "axios": "^1.7.7",
    "clipboard-copy": "^4.0.1",
    "csv-stringify": "^5.5.3",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.2.3",
    "deepmerge": "^4.3.1",
    "file-saver-es": "^2.0.5",
    "flat": "^6.0.1",
    "is-valid-domain": "^0.1.6",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "lodash": "^4.17.21",
    "mitt": "3.0.1",
    "path-browserify": "^1.0.1",
    "plyr": "^3.7.8",
    "query-string": "^9.1.1",
    "sortablejs": "^1.15.3",
    "vue-i18n": "^11.0.1",
    "vue-multiselect": "^3.1.0",
    "vue-observe-visibility": "^2.0.0-alpha.1",
    "vue-router": "^4.4.5",
    "webitel-sdk": "^24.10.29",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.12.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/compat": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "globby": "^14.0.2",
    "happy-dom": "^15.11.6",
    "markdown-it": "^14.1.0",
    "markdown-table": "^3.0.4",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "postcss-prefix-selector": "^2.1.0",
    "prettier": "^3.4.2",
    "prismjs": "^1.29.0",
    "sass": "^1.81.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.22.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.4.11",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-static-copy": "^2.1.0",
    "vite-plugin-svg-sprite": "^0.5.2",
    "vite-plugin-vue-docgen": "^0.3.4",
    "vitepress": "1.5.0",
    "vitest": "^2.1.5",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10",
    "vuex": "^4.1.0",
    "pinia": "^2.3.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "engines": {
    "npm": "10",
    "node": "v22"
  }
}
