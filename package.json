{
  "name": "@webitel/ui-sdk",
  "version": "2.1.16",
  "private": false,
  "scripts": {
    "serve-lib": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name ui-sdk ./src/install.js",
    "build:app": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "publish-lib": "npm run test:unit && npm run build && npm publish --access public",
    "publish-lib:testless": "npm run build && npm publish --access public",
    "prepare": "husky install"
  },
  "main": "./dist/@webitel/ui-sdk.common.js",
  "files": [
    "dist/*",
    "src/locale/*",
    "src/css/*",
    "src/components/*",
    "src/enums/*",
    "src/mixins/*",
    "src/scripts/*",
    "src/modules/*",
    "src/store/*",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "clipboard-copy": "^4.0.1",
    "core-js": "^3.6.5",
    "csv-stringify": "^5.5.3",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "file-saver": "^2.0.2",
    "floating-vue": "^1.0.0-beta.17",
    "jszip": "^3.5.0",
    "jszip-utils": "^0.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "plyr": "3.6.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.18.2",
    "vue-multiselect": "^2.1.6",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^3.2.0",
    "vuejs-datepicker": "^1.6.2",
    "webitel-sdk": "^0.1.157"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "svg-sprite-loader": "^6.0.11",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.6.2"
  }
}
