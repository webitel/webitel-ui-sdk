		"jszip-utils": "^0.1.0",
		"lint-staged": "^16.x",
		"markdown-it": "^14.1.0",
		"markdown-table": "^3.0.4",
		"path": "^0.12.7",
		"pinia": "^3.x",
		"postcss": "^8.5.3",
		"postcss-prefix-selector": "^2.1.0",
		"prismjs": "^1.30.0",
		"publint": "^0.3.16",
		"sass": "^1.85.1",
