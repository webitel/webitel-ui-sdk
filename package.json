		"vitepress": "^1.6.4",
		"vitest": "^4.0.x",
		"vue": "^3.5.x",
		"vue-tsc": "^3.x",
		"vuex": "^4.1.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "4.9.5"
	},
	"overrides": {
		"date-fns": "^4.1.0"
	},
	"engines": {
		"npm": "11",
		"node": "v24"
	},
