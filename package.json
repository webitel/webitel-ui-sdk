{
  "name": "@webitel/ui-sdk",
  "version": "25.6.38",
  "private": false,
  "scripts": {
    "dev": "npm run docs:dev",
    "build": "vite build",
    "test:unit": "vitest",
    "build:types": "vue-tsc -p ./tsconfig.build.json",
    "publish-lib": "npm run build && (npm run build:types || true) && npm publish --access public",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "lint:fix": "eslint --fix ./src && prettier --write --check ./src",
    "lint:fix-staged": "npx lint-staged",
    "lint:package": "publint",
    "utils:link": "npm link",
    "utils:update-node": "nvm install --lts && nvm alias default node",
    "utils:i": "npm install"
  },
  "workspaces": [
    "./packages/api-services"
  ],
  "main": "./dist/ui-sdk.js",
  "type": "module",
  "types": "./dist/types",
  "exports": {
    ".": "./dist/ui-sdk.js",
    "./store": "./src/store/new/index",
    "./store*": "./src/store*",
    "./src/store*": "./src/store*",
    "./enums": "./src/enums/index",
    "./enums*": "./src/enums*",
    "./src/enums*": "./src/enums*",
    "./components": "./src/components/index",
    "./components*": "./src/components*",
    "./src/components*": "./src/components*",
    "./scripts": "./src/scripts/index",
    "./scripts*": "./src/scripts*",
    "./src/scripts*": "./src/scripts*",
    "./api*": "./src/api*",
    "./src/api*": "./src/api*",
    "./locale*": "./src/locale*",
    "./src/locale*": "./src/locale*",
    "./composables*": "./src/composables*",
    "./src/composables*": "./src/composables*",
    "./css*": "./src/css*",
    "./src/css*": "./src/css*",
    "./directives*": "./src/directives*",
    "./src/directives*": "./src/directives*",
    "./mixins*": "./src/mixins*",
    "./src/mixins*": "./src/mixins*",
    "./validators*": "./src/validators*",
    "./src/validators*": "./src/validators*",
    "./modules*": "./src/modules*",
    "./src/modules*": "./src/modules*",
    "./plugins*": "./src/plugins*",
    "./src/plugins*": "./src/plugins*",
    "./tests*": "./src/tests*",
    "./src/tests*": "./src/tests*",
    "./dist*": "./dist*"
  },
  "files": [
    "dist/*",
    "src/api/*",
    "src/assets/*",
    "src/locale/*",
    "src/composables/*",
    "src/css/*",
    "src/components/*",
    "src/directives/*",
    "src/enums/*",
    "src/mixins/*",
    "src/scripts/*",
    "src/validators/*",
    "src/modules/*",
    "src/plugins/*",
    "src/store/*",
    "src/tests/*",
    "src/install.ts",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@morev/vue-transitions": "^3.0.5",
    "@primeuix/themes": "^1.1.1",
    "@tailwindcss/vite": "^4.1.7",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^4.5.1",
    "@vueuse/components": "^13.0.0",
    "@webitel/api-services": "^0.0.19",
    "@webitel/styleguide": "^24.12.26",
    "autosize": "^6.0.1",
    "axios": "^1.8.3",
    "clipboard-copy": "^4.0.1",
    "csv-stringify": "^5.5.3",
    "date-fns": "^4.1.0",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.2.3",
    "deepmerge": "^4.3.1",
    "file-saver-es": "^2.0.5",
    "flat": "^6.0.1",
    "is-valid-domain": "^0.1.6",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "path-browserify": "^1.0.1",
    "plyr": "^3.7.8",
    "primevue": "^4.3.4",
    "query-string": "^9.1.1",
    "sortablejs": "^1.15.3",
    "tailwindcss": "^4.1.7",
    "vue-i18n": "^11.1.2",
    "vue-multiselect": "^3.1.0",
    "vue-observe-visibility": "^2.0.0-alpha.1",
    "vue-router": "^4.5.0",
    "webitel-sdk": "^25.5.4",
    "xlsx": "0.18.5",
    "zod": "^3.25.55"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compat": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "markdown-it": "^14.1.0",
    "markdown-table": "^3.0.4",
    "path": "^0.12.7",
    "pinia": "^3.0.1",
    "postcss": "^8.5.3",
    "postcss-prefix-selector": "^2.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prismjs": "^1.30.0",
    "publint": "^0.3.9",
    "sass": "^1.85.1",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-auto-import": "^19.1.1",
    "vite": "^6.2.2",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-svg-sprite": "=0.5",
    "vite-plugin-vue-docgen": "^0.3.4",
    "vitepress": "1.6.3",
    "vitest": "^3.0.9",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8",
    "vuex": "^4.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "overrides": {
    "vite": "^6.2.2",
    "date-fns": "^4.1.0"
  },
  "engines": {
    "npm": "10",
    "node": "v22"
  }
