{
  "name": "@webitel/ui-sdk",
  "version": "25.4.38",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vitest",
    "lint:fix": "prettier --write --check . && eslint --fix",
    "publish-lib": "npm run test:unit && npm run build && npm publish --access public",
    "publish-lib:testless": "npm run build && npm publish --access public",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "tsc": "tsc -p tsconfig.json",
    "link": "npm link",
    "update-node": "nvm install --lts && nvm alias default node",
    "i": "npm install",
    "lint:package": "publint",
    "orval": "orval",
    "prepare": "husky && husky install"
  },
  "main": "./dist/ui-sdk.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/ui-sdk.js"
    },
    "./store": {
      "import": "./src/store/new/index.js"
    },
    "./enums": {
      "import": "./src/enums/index.js"
    },
    "./components": {
      "import": "./src/components/index.js"
    },
    "./scripts": {
      "import": "./src/scripts/index.js"
    },
    "./dist*": {
      "import": "./dist*"
    },
    "./src/api*": {
      "import": "./src/api*"
    },
    "./src/locale*": {
      "import": "./src/locale*"
    },
    "./src/composables*": {
      "import": "./src/composables*"
    },
    "./src/css*": {
      "import": "./src/css*"
    },
    "./src/components*": {
      "import": "./src/components*"
    },
    "./src/directives*": {
      "import": "./src/directives*"
    },
    "./src/enums*": {
      "import": "./src/enums*"
    },
    "./src/mixins*": {
      "import": "./src/mixins*"
    },
    "./src/scripts*": {
      "import": "./src/scripts*"
    },
    "./src/validators*": {
      "import": "./src/validators*"
    },
    "./src/modules*": {
      "import": "./src/modules*"
    },
    "./src/plugins*": {
      "import": "./src/plugins*"
    },
    "./src/store*": {
      "import": "./src/store*"
    },
    "./src/tests*": {
      "import": "./src/tests*"
    }
  },
  "files": [
    "dist/*",
    "src/api/*",
    "src/assets/*",
    "src/locale/*",
    "src/composables/*",
    "src/css/*",
    "src/components/*",
    "src/directives/*",
    "src/enums/*",
    "src/mixins/*",
    "src/scripts/*",
    "src/validators/*",
    "src/modules/*",
    "src/plugins/*",
    "src/store/*",
    "src/tests/*",
    "src/install.ts",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@morev/vue-transitions": "^3.0.5",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^4.5.1",
    "@vueuse/components": "^13.0.0",
    "@webitel/styleguide": "^24.12.26",
    "autosize": "^6.0.1",
    "axios": "^1.8.3",
    "clipboard-copy": "^4.0.1",
    "csv-stringify": "^5.5.3",
    "date-fns": "^4.1.0",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.2.3",
    "deepmerge": "^4.3.1",
    "file-saver-es": "^2.0.5",
    "flat": "^6.0.1",
    "is-valid-domain": "^0.1.6",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "path-browserify": "^1.0.1",
    "plyr": "^3.7.8",
    "query-string": "^9.1.1",
    "sortablejs": "^1.15.3",
    "vue-i18n": "^11.1.2",
    "vue-multiselect": "^3.1.0",
    "vue-observe-visibility": "^2.0.0-alpha.1",
    "vue-router": "^4.5.0",
    "webitel-sdk": "^25.2.11",
    "xlsx": "0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compat": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "markdown-it": "^14.1.0",
    "markdown-table": "^3.0.4",
    "orval": "^7.6.0",
    "path": "^0.12.7",
    "pinia": "^3.0.1",
    "postcss": "^8.5.3",
    "postcss-prefix-selector": "^2.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prismjs": "^1.30.0",
    "publint": "^0.3.9",
    "sass": "^1.85.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-auto-import": "^19.1.1",
    "vite": "^6.2.2",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-svg-sprite": "=0.5",
    "vite-plugin-vue-docgen": "^0.3.4",
    "vitepress": "1.6.3",
    "vitest": "^3.0.9",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8",
    "vuex": "^4.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "overrides": {
    "vite": "^6.2.2",
    "date-fns": "^4.1.0"
  },
  "engines": {
    "npm": "10",
    "node": "v22"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
