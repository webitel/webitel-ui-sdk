		"path-browserify": "^1.0.1",
		"primevue": "^4.5.4",
		"qs-esm": "^7.0.3",
		"sortablejs": "^1.15.3",
		"tailwindcss": "^4.1.7",
		"vue-multiselect": "^3.4.0",
		"vue-observe-visibility": "^2.0.0-alpha.1",
		"xlsx": "0.18.5"
	},
	"peerDependencies": {
		"@vuelidate/core": "^2.0.3",
		"@vuelidate/validators": "^2.0.4",
		"@webitel/api-services": ">=0.0.105",
		"@webitel/styleguide": "~26.2",
		"axios": "^1.8.3",
		"date-fns": "^4.1.0",
		"deep-copy": "^1.4.2",
		"deep-equal": "^2.2.3",
		"deepmerge": "^4.3.1",
		"jszip": "^3.10.1",
		"jszip-utils": "^0.1.0",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.1",
		"path-browserify": "^1.0.1",
		"vidstack": "^1.12.13",
		"vue-i18n": "^11.1.2",
		"vue-router": "^4.5.0",
		"webitel-sdk": "^26.1.3",
		"zod": "^4.x"
	},
