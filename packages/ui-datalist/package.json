{
  "name": "@webitel/ui-datalist",
  "version": "1.0.9",
  "description": "Toolkit for building data lists in webitel ui system",
  "scripts": {
    "build:types": "vue-tsc -p ./tsconfig.build.json",
    "lint:fix": "eslint --fix ./src && prettier --write --check ./src",
    "lint:fix-staged": "npx lint-staged",
    "lint:package": "publint",
    "utils:i": "npm i",
    "utils:link": "npm link",
    "utils:publish": "npm publish --access public"
  },
  "type": "module",
  "main": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "workspaces": [
    "../../"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./filters": "./src/modules/filters/index.ts",
    "./presets": "./src/modules/filter-presets/index.ts"
  },
  "files": [
    "src/*",
    "Readme.md",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "vue": "^3.5",
    "axios": "^1",
    "pinia": "^3.x",
    "date-fns": "^4"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@webitel/ui-sdk": "^25.4.59",
    "@webitel/styleguide": "^24.12.26"
  },
  "devDependencies": {
    "vue-tsc": "^2.2.8",
    "vite-plugin-checker": "^0.9.0",
    "sass": "^1.85.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-auto-import": "^19.1.1",
    "publint": "^0.3.9",
    "vite": "^6.2.4",
    "@eslint/js": "^9.22.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.10",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "lint-staged": "^15.5.0",
    "@vitejs/plugin-vue": "^5.2.3"
  },
  "engines": {
    "npm": "10",
    "node": "v22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webitel/webitel-ui-sdk"
  },
  "keywords": [
    "webitel",
    "webitel-ui"
  ],
  "author": "webitel",
  "license": "ISC"
}
