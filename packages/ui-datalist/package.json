{
  "name": "@webitel/ui-datalist",
  "version": "1.0.80",
  "description": "Toolkit for building data lists in webitel ui system",
  "scripts": {
    "make-all": "npm version patch --git-tag-version false && ( npm run lint:fix || true) && (npm run build:types || true) && npm run utils:publish",
    "build:types": "vue-tsc -p ./tsconfig.build.json",
    "lint:fix": "eslint --fix ./src && prettier --write --check ./src",
    "lint:fix-staged": "npx lint-staged",
    "lint:package": "publint",
    "utils:i": "npm i",
    "utils:link": "npm link",
    "utils:publish": "npm publish --access public"
  },
  "type": "module",
  "main": "./src/index.ts",
  "types": "./types/index.d.ts",
  "workspaces": [
    "../../",
    "../api-services"
  ],
  "files": [
    "src/*",
    "types/",
    "Readme.md",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "axios": "^1",
    "date-fns": "^4",
    "pinia": "^3.x",
    "vue": "^3.5"
  },
  "dependencies": {
    "@regle/schemas": "^1.2.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@webitel/styleguide": "^24.12.61",
    "@webitel/ui-sdk": "^25.8.62",
    "@webitel/api-services": "^0.x",
    "zod": "^3.25.55",
    "@vueuse/core": "^13.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@standard-schema/spec": "^1.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "publint": "^0.3.9",
    "sass": "^1.85.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-auto-import": "^19.1.1",
    "vite": "^6.2.4",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "npm": ">=10",
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webitel/webitel-ui-sdk"
  },
  "keywords": [
    "webitel",
    "webitel-ui"
  ],
  "author": "webitel",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.ts"
    },
    "./filters": {
      "types": "./types/modules/filters/index.d.ts",
      "import": "./src/modules/filters/index.ts"
    },
    "./presets": {
      "types": "./types/modules/filter-presets/index.d.ts",
      "import": "./src/modules/filter-presets/index.ts"
    },
    "./card": {
      "types": "./types/modules/card/index.d.ts",
      "import": "./src/modules/card/index.ts"
    }
  }
}
