{
	"name": "@webitel/ui-chats",
	"version": "0.0.14",
	"description": "Reusable Webitel Frontend Code for Chats UI",
	"workspaces": [
		"../../",
		"../api-services"
	],
	"scripts": {
		"dev": "vite",
		"make-all": "npm version patch --git-tag-version false && (npm run lint:biome || true) && (npm run build:types || true) && npm run utils:publish",
		"build": "(npm run build:types || true)",
		"build:types": "vue-tsc -p ./tsconfig.build.json",
		"lint:biome": "biome check --write",
		"utils:publish": "npm publish --access public"
	},
	"dependencies": {
		"autolinker": "^4.1.5",
		"insert-text-at-cursor": "^0.3.0",
		"mitt": "^3.0.1",
		"vue": "^3.5.24"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/node": "^24.10.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/tsconfig": "^0.8.1",
		"@webitel/styleguide": "^24.12.95",
		"@webitel/ui-sdk": "~25.12",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.2.2",
		"vue-tsc": "^3.1.3"
	},
	"type": "module",
	"main": "index.ts",
	"types": "index.d.ts",
	"files": [
		"src/*",
		"types/*",
		"Readme.md",
		"CHANGELOG.md"
	],
	"exports": {
		"./ui": {
			"types": "./types/ui/index.d.ts",
			"import": "./src/ui/index.ts"
		}
	},
	"author": "webitel",
	"license": "ISC",
	"keywords": [
		"webitel",
		"webitel-ui"
	],
	"repository": {
		"type": "git",
		"url": "github.com/webitel/webitel-ui-sdk"
	},
	"engines": {
		"npm": ">=10",
		"node": ">=v22"
	}
}
